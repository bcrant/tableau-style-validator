# TABLE XML SKELETON
<workbook>
    <actions>   # Ignore for now
    <worksheets>
        <worksheet> # Has attribute name
            <layout-options>
                <title>
                    <formatted-text>
                </title>
            </layout-options>
            <table>
                <style>
                      <style-rule element='axis'>
                        <format attr='title' class='0' field='' scope='cols' value='Compensation ($)' />
                        <format attr='subtitle' class='0' field='' scope='cols' value='' />
                        <format attr='auto-subtitle' class='0' field='' scope='cols' value='true' />
                      </style-rule>
                      <style-rule element='header'>
                        <format attr='width' field='' value='136' />
                      </style-rule>
                      <style-rule element='label'>
                        <format attr='display' field='' value='false' />
                        <format attr='text-format' field='' value='n#,##0,K;-#,##0,K' />
                      </style-rule>
                      <style-rule element='worksheet'>
                        <format attr='display-field-labels' scope='rows' value='false' />
                      </style-rule>
                      <style-rule element='refline'>
                        <format attr='stroke-color' id='refline2' value='#acacac' />
                        <format attr='color' id='refline2' value='#000000' />
                        <format attr='color' id='refline3' value='#000000' />
                        <format attr='stroke-color' id='refline4' value='#acacac' />
                        <format attr='color' id='refline4' value='#000000' />
                      </style-rule>
                      <style-rule element='legend'>
                        <format attr='background-color' value='#ffffff' />
                        <format attr='col-width' field='' value='240' />
                      </style-rule>
                      <style-rule element='legend-title-text'>
                        <format attr='color' field='' value=''>
                          <formatted-text />
                        </format>
                      </style-rule>
                    </style>
                    <panes>
                        <pane>
                            <customized-tooltip>
                              <formatted-text>
                                <run fontcolor='#d3872a' fontname='Tableau Bold' fontsize='11'></run>
                                <run fontcolor='#666666' fontname='Tableau Medium' fontsize='9'></run>
                                <run fontcolor='#000000' fontsize='9'>Æ </run>
                                <run fontcolor='#666666' fontsize='9'></run>
                                <run fontcolor='#000000' fontsize='9'></run>
                                <run fontcolor='#000000' fontname='Tableau Semibold' fontsize='9'></run>
                                <run fontcolor='#555555' fontsize='9'></run>
                                <run fontcolor='#000000' fontsize='9'></run>
                                <run fontcolor='#000000' fontname='Tableau Semibold' fontsize='9'></run>
                              </formatted-text>
                            </customized-tooltip>
                            <style>
                                <style-rule>
                                </style-rule>
                            </style>
                        </pane>
                    </panes>
                </table>
        </worksheet>
        #
        #
        # Many <worksheet> elements
        #
        #
    </worksheets>
    <dashboards>
        <dashboard>
            <layout-options>
                <title>
                    <formatted-text>
                        <run />
                    </formatted-text>
                </title>
            </layout-options>
            <style>
            </style>
            <size minheight='620' minwidth='1000' />
        </dashboard>
    </dashboards>
    <windows>
        # Might be able to get Mark Colors here
        # Ignore for now
    </windows>
</workbook>


